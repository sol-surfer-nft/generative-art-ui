    "react-draft-wysiwyg": "^1.14.7",
    "react-drag-listview": "^0.1.8",
    "react-drawer": "^1.3.4",
    "react-dropzone": "^11.3.2",
    "react-dual-listbox": "^2.1.2",
    "react-flatpickr": "^3.10.7",
    "react-google-login": "^5.2.2",
    "react-hook-form": "^7.15.3",
    "react-i18next": "^11.10.0",
