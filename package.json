    "react-bootstrap-sweetalert": "^5.2.0",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-editor": "^1.4.0",
    "react-color": "^2.19.3",
    "react-countdown": "^2.3.2",
    "react-datepicker": "^4.1.1",
