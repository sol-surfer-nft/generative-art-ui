    "availity-reactstrap-validation": "^2.7.1",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.20.0",
    "bootstrap": "^5.1.0",
    "classnames": "^2.3.1",
    "eslint": "^7.29.0",
    "firebase": "^8.6.3",
    "i18next": "^20.3.1",
    "i18next-browser-languagedetector": "^6.1.1",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "node-sass": "^6.0.1",
